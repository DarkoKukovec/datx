{
  "name": "datx",
  "devDependencies": {
    "@infinum/eslint-plugin": "^1.0.0",
    "eslint": "^8.45.0",
    "husky": "^8.0.3",
    "lerna": "^7.1.4",
    "prettier": "2.8.8",
    "typescript": "5.1.6"
