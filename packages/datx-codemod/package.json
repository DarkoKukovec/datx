{
  "name": "@datx/codemod",
  "version": "2.5.1",
  "description": "DatX codemods",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinum/datx.git"
  },
  "bugs": {
    "url": "https://github.com/infinum/datx/issues"
  },
  "homepage": "https://github.com/infinum/datx#readme",
  "author": "Infinum JavaScript Team <hello@infinum.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "4.1.0",
    "cheerio": "1.0.0-rc.9",
    "execa": "4.0.3",
    "globby": "11.0.1",
    "inquirer": "7.3.3",
    "is-git-clean": "1.1.0",
    "jscodeshift": "0.13.1",
    "meow": "7.0.1"
  },
  "files": [
    "transforms/*.js",
    "bin/*.js",
    "lib/**/*.js",
    "lib/cra-to-next/gitignore"
  ],
  "scripts": {
    "build": "tsc -d -p tsconfig.json",
    "prepublishOnly": "cd ../../ && turbo run build",
    "dev": "tsc -d -w -p tsconfig.json",
    "test": "jest"
  },
  "bin": "./bin/datx-codemod.js",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/jscodeshift": "0.11.0",
    "jest": "^29.5.0",
    "typescript": "~4.7.3"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/transforms/__tests__",
      "/transforms/__testfixtures__",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "transforms/__tests__/(.*).test.ts$",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "preset": "ts-jest",
    "testMatch": null,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
