{
  "name": "datx",
  "version": "0.9.1",
  "description": "A MobX data store",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinum/datx.git"
  },
  "bugs": {
    "url": "https://github.com/infinum/datx/issues"
  },
  "homepage": "https://github.com/infinum/datx#readme",
  "author": "Infinum JavaScript Team <hello@infinum.co>",
  "license": "MIT",
  "devDependencies": {
    "@infinumjs/tslint-config": "^1.1.1",
    "@types/jest": "^22.1.2",
    "@types/node": "^9.4.4",
    "jest": "^22.2.2",
    "mobx": "^3.5.1",
    "mobx-collection-store": "^2.0.0",
    "rollup": "^0.56.2",
    "rollup-plugin-typescript2": "^0.11.1",
    "rollup-plugin-uglify": "^3.0.0",
    "ts-jest": "^22.0.3",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "peerDependencies": {
    "mobx": "^3.5.1"
  },
  "scripts": {
    "test": "jest --coverage",
    "watch": "jest --watch --coverage",
    "benchmark": "jest benchmark",
    "benchmark-watch": "jest benchmark --watch",
    "lint": "tslint --project .",
    "prepublish": "npm run build",
    "build": "rollup -c"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/test/",
      "/node_modules/",
      "../datx-utils/dist"
    ],
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "test/(.*).ts",
    "transform": {
      ".ts": "ts-jest"
    }
  },
  "dependencies": {
    "datx-utils": "^0.9.0"
  }
}
