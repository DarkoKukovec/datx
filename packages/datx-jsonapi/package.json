{
  "name": "datx-jsonapi",
  "version": "1.0.1",
  "description": "DatX mixin for JSON API support",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinum/datx.git"
  },
  "bugs": {
    "url": "https://github.com/infinum/datx/issues"
  },
  "homepage": "https://github.com/infinum/datx#readme",
  "author": "Infinum JavaScript Team <hello@infinum.co>",
  "license": "MIT",
  "keywords": [
    "datx",
    "mobx",
    "jsonapi"
  ],
  "devDependencies": {
    "@infinumjs/tslint-config": "^2.0.0-beta.6",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/nock": "^11.1.0",
    "@types/node": "^13.1.6",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "mobx": "^5.15.1",
    "nock": "^11.7.2",
    "rollup": "^1.29.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "mobx": "^4.2.0 || ^5.5.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "watch": "jest --watch --coverage",
    "lint": "tslint --project .",
    "prepublish": "npm run build",
    "build": "rollup -c"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/test/",
      "/node_modules/",
      "../datx/dist",
      "../datx-utils/dist"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "test/main.ts",
    "transform": {
      ".ts": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    }
  },
  "dependencies": {
    "datx": "^2.0.0",
    "datx-utils": "^2.0.0"
  }
}
