    "test": "jest --coverage",
    "watch": "jest --watch --coverage",
    "prepublish": "npm run build",
    "build": "rollup -c"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/test/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "test/(.*).test.ts$",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "preset": "ts-jest",
    "testMatch": null,
    "setupFilesAfterEnv": [
      "./test/setup.ts"
    ]
  },
  "dependencies": {
    "@datx/core": "2.3.1",
    "@datx/jsonapi": "2.3.1",
    "@datx/utils": "2.3.1"
  }
